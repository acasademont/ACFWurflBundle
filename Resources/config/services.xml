<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="acf_wurfl.container.class">ScientiaMobile\WURFL\Container\Container</parameter>
        <parameter key="acf_wurfl.engine.class">ScientiaMobile\WURFL\WURFLEngine</parameter>
        <parameter key="acf_wurfl.storage_factory.class">ScientiaMobile\WURFL\Storage\StorageFactory</parameter>
        <parameter key="acf_wurfl.cache_factory.class">ScientiaMobile\WURFL\Cache\CacheFactory</parameter>
    </parameters>

    <services>
        <service id="acf_wurfl.storage_factory" class="%acf_wurfl.storage_factory.class%" public="false"/>
        <service id="acf_wurfl.cache_factory" class="%acf_wurfl.cache_factory.class%" public="false"/>
        <service id="acf_wurfl.container" class="%acf_wurfl.container.class%" public="false"/>
        <service id="acf_wurfl.engine" class="%acf_wurfl.engine.class%" public="false">
            <argument type="service" id="acf_wurfl.container"/>
        </service>
    </services>

</container>

